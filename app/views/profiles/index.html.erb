<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Second name</th>
      <th>First last name</th>
      <th>Second last name</th>
      <th>Birth date</th>
      <th>Curp</th>
      <th>Rfc</th>
      <th>Gender</th>
      <th>Birth state</th>
      <th>Phone number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.first_name %></td>
        <td><%= profile.second_name %></td>
        <td><%= profile.first_last_name %></td>
        <td><%= profile.second_last_name %></td>
        <td><%= profile.birth_date %></td>
        <td><%= profile.curp %></td>
        <td><%= profile.rfc %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.birth_state %></td>
        <td><%= profile.phone_number %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
