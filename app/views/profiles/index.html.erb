<h1>Listing Profiles</h1>
<table>
  <thead>
    <tr>
      <th><%= trans(:profile, :first_name)%></th>
      <th><%= trans(:profile, :second_name) %></th>
      <th><%= trans(:profile, :first_last_name) %></th>
      <th><%= trans(:profile, :second_last_name) %></th>
      <th><%= trans(:profile, :birth_date) %></th>
      <th><%= trans(:profile, :curp) %></th>
      <th><%= trans(:profile, :rfc) %></th>
      <th><%= trans(:profile, :email) %></th>
      <th><%= trans(:profile, :gender) %></th>
      <th><%= trans(:profile, :birth_date) %></th>
      <th><%= trans(:profile, :phone_number) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.first_name %></td>
        <td><%= profile.second_name %></td>
        <td><%= profile.first_last_name %></td>
        <td><%= profile.second_last_name %></td>
        <td><%= profile.birth_date %></td>
        <td><%= profile.curp %></td>
        <td><%= profile.rfc %></td>
        <td><%= profile.email %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.birth_state %></td>
        <td><%= profile.phone_number %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

